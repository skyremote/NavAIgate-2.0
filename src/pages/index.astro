---
import BaseLayout from '../layouts/BaseLayout.astro';
import NavBar from '../components/NavBar';
import Hero from '../components/Hero';
import AboutSection from '../components/AboutSection';
import ClientsSection from '../components/ClientsSection';
import EntepriseSection from '../components/EntepriseSection';
import ContactSection from '../components/ContactSection';
import Footer from '../components/Footer';
---

<BaseLayout title="NavAIgate - AI Consultancy | Custom AI Tools & Ente-prise">
  <!-- Navigation -->
  <NavBar client:load />

  <!-- Hero Section -->
  <Hero client:load />

  <!-- About Section -->
  <AboutSection client:load />

  <!-- Clients Section -->
  <ClientsSection client:load />

  <!-- Ente-prise Product Section -->
  <EntepriseSection client:load />

  <!-- Contact Section -->
  <ContactSection client:load />

  <!-- Footer -->
  <Footer client:load />
</BaseLayout>
